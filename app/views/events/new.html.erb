<h1>Create new event</h1>
<%= form_for @event, html: { multipart: true } do |f| %>
 <%= f.label :name %><br>
 <%= f.text_field :name%><br>
 <%= f.label :description %><br>
 <%= f.text_area :description %><br>
 <%= f.file_field :image %><br><br>
 <%= f.submit %>
<% end %>
<%= link_to 'Home', root_path %>
