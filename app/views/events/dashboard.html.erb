<% if coordinator_signed_in? %>
 <%= link_to "Signout", destroy_coordinator_session_path, method: :delete %>     <!-- Session Destroy of Co-ordinator -->
 | <%= link_to 'New Event', new_event_path %>
 <% end %>
<% @events.each do |event| %>
 <h1><%= event.name %></h1>
 <h3><%= event.description %></h3>
 <%= link_to "Edit", edit_event_path(event) %> |
 <%= link_to "Destroy", event_path(event), method: :delete, data:{confirm: "Are you sure?"} %>  <!-- Session Destroy alert  -->
<% end %>
