<%= link_to "coordinator login", new_coordinator_session_path , style: "float: right"%>
<h1 style="color: orange">Welcome Page</h1>
<% @events.each do |event| %>
 <!-- <%= link_to 'PDF', root_path(format: 'pdf') %> -->         <!-- Output PDF File through the current path -->
 <h1><%= event.name %></h1>
 <h3><%= event.description %></h3>
 <% (0...event.images.count).each do |image| %>               <!-- Displaying Uploaded image look at models/event.rb -->
    <%= image_tag (event.small(image)) %><br>             <!-- Images display after resizing models/event.rb -> small method -->
  <% end %>
<% end %>
